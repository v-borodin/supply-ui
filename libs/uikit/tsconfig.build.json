  "exclude": [
    "libs/**/test.ts",
    "**/*.spec.ts"
  ],
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "declaration": true,
    "declarationMap": true,
    "inlineSources": true,
    "types": [],
    "paths": {
      "@supply/cdk/*": [
